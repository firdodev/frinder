<?xml version="1.0" encoding="utf-8"?>
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="/frinder-logo.png"/>
      <square150x150logo src="/frinder-logo.png"/>
      <square310x310logo src="/frinder-logo.png"/>
      <wide310x150logo src="/frinder-logo.png"/>
      <TileColor>#ed8c00</TileColor>
    </tile>
    <notification>
      <polling-uri src="https://frinder.co/notifications/feed"/>
      <frequency>30</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
